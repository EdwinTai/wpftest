<Window x:Class="WpfAppDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="ChromeButton" Content="Open Google in Chrome" HorizontalAlignment="Left" Margin="189,152,0,0" VerticalAlignment="Top" Click="ChromeButton_Click"/>
        <Button x:Name="EdgeButton" Content="Open Google in Edge" HorizontalAlignment="Left" Margin="473,152,0,0" VerticalAlignment="Top" Click="EdgeButton_Click"/>
        <Button x:Name="checkupdate" Content="AutoUpdate.NET" HorizontalAlignment="Left" Margin="236,84,0,0" VerticalAlignment="Top" ButtonBase.Click="checkupdate_Click"/>
        <Button x:Name="WinSparkle" Content="WinSparkle Update" HorizontalAlignment="Left" Margin="506,84,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>
